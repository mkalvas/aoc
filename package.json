{
  "name": "aoc",
  "version": "1.2.0",
  "author": "mkalvas",
  "main": "bin/aoc.js",
  "bin": {
    "aoc": "./bin/aoc.js"
  },
  "files": [
    "bin",
    "build"
  ],
  "scripts": {
    "build": "babel src --out-dir build --copy-files",
    "build:watch": "babel src --watch --out-dir build --copy-files",
    "test": "jest",
    "test:e2e": "jest --testPathIgnorePatterns /bin /build /node_modules/",
    "clean": "rm -rf build",
    "dev": "npm run clean && npm run build:watch",
    "start": "node bin/aoc.js run",
    "obfuscate": "terser"
  },
  "dependencies": {
    "chokidar": "^3.5.2",
    "terser": "^5.10.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1"
  },
  "license": "MIT"
}
