{
  "name": "aoc-2020",
  "version": "0.14.0",
  "scripts": {
    "build": "babel src --out-dir build --copy-files",
    "build:watch": "babel src --watch --out-dir build --copy-files",
    "test": "jest",
    "clean": "rm -rf build",
    "dev": "npm run clean && npm run build:watch",
    "start": "node build/index.js",
    "time": "node build/timings.js",
    "cloc": "grep -Rcv '^s*$' --include='code.js' ./src"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2"
  },
  "license": "MIT"
}
